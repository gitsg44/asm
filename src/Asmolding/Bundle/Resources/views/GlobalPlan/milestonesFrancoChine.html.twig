{# empty Twig template #}

{% for milestone in milestones %}
    {% if milestone.solution == "FRANCO-CHINE" %}
        <a href="{{ path('asmolding_admin_manageMilestones', {'id':milestone.id}) }}"><li class="ui-state-default"><span class="ui-icon ui-icon-arrowthick-2-n-s"></span>
            {{milestone.name}}
            <input type="text" readonly="true" value="{{milestone.sequence}}" class="ui-state-default inline" id="posInput"/>
        </li></a>
    {% endif %}
{% endfor %}

