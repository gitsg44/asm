{# empty Twig template #}
<table>
{% for c in companies %}
    <tr>
	<td>{{ c.name }}</td>
	<td><a href="{{ path('asmolding_admin_filterByCriteria', {'companySeparator':'/','companyId':c.id, 'contactSeparator':'/', 'contactId': '', 'projectSeparator': '/', 'projectId': '', 'dasSeparator': '/', 'das': '', 'solutionSeparator': '/', 'solution':''}) }}">Sélectionner</a></td>
{% else %}
    <tr>
	<td>Aucun client n'a été trouvé.</td>
    </tr>
{% endfor %}
</table>
